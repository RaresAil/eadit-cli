{
  "name": "@adr-express-ts/cli",
  "version": "7.0.2",
  "description": "CLI for '@adr-express-ts/core'",
  "main": "bin/index.js",
  "preferGlobal": true,
  "bin": {
    "adr-cli": "bin/commands.js"
  },
  "types": "bin",
  "files": [
    "bin/*"
  ],
  "scripts": {
    "build": "npm run lint && rm -rf ./bin/ && tsc -p . && cp -r ./src/templates ./bin/templates && rm -r ./bin/__tests__",
    "lint": "eslint src/**/*.ts",
    "version": "npm run build",
    "test": "npm run build && mocha",
    "dev": "NODE_ENV=dev nodemon src/commands.ts",
    "bin-dev": "npm run build && NODE_ENV=dev node bin/commands.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RaresAil/eadit-cli.git"
  },
  "keywords": [
    "CLI",
    "typescript",
    "cli",
    "TypeScript",
    "ES2017",
    "ES6",
    "mvc",
    "adr",
    "dependency",
    "injection"
  ],
  "author": "Rares Designs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RaresAil/eadit-cli/issues"
  },
  "homepage": "https://github.com/RaresAil/eadit-cli#readme",
  "devDependencies": {
    "@types/async-lock": "^1.1.3",
    "@types/chai": "^4.2.21",
    "@types/concat-stream": "^1.6.1",
    "@types/cross-spawn": "^6.0.2",
    "@types/inquirer": "^7.3.3",
    "@types/mocha": "^8.2.3",
    "@types/node": "^16.7.0",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "chai": "^4.3.4",
    "concat-stream": "^2.0.0",
    "cross-spawn": "^7.0.3",
    "eslint": "^7.30.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^5.0.0",
    "mocha": "^9.0.2",
    "nodemon": "^2.0.12",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "async-lock": "^1.3.0",
    "colors": "^1.4.0",
    "commander": "^8.0.0",
    "inquirer": "^8.1.2",
    "re2": "^1.16.0"
  }
}
