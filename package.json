{
  "name": "eadit-cli",
  "version": "6.1.0",
  "description": "CLI for 'adr-express-ts'",
  "main": "bin/index.js",
  "preferGlobal": true,
  "bin": {
    "eadit-cli": "bin/commands.js"
  },
  "types": "bin",
  "files": [
    "bin/*"
  ],
  "scripts": {
    "build": "eslint src/**/*.ts && rm -rf ./bin/ && tsc -p . && cp -r ./src/templates ./bin/templates",
    "lint": "eslint src/**/*.ts",
    "version": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RaresAil/eadit-cli.git"
  },
  "keywords": [
    "CLI",
    "typescript",
    "cli",
    "TypeScript",
    "ES2017",
    "ES6",
    "mvc",
    "adr",
    "dependency",
    "injection"
  ],
  "author": "Rares Designs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RaresAil/eadit-cli/issues"
  },
  "homepage": "https://github.com/RaresAil/eadit-cli#readme",
  "devDependencies": {
    "@types/async-lock": "^1.1.2",
    "@types/inquirer": "^7.3.1",
    "@types/node": "^14.14.22",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "eslint": "^7.18.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^5.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "async-lock": "^1.2.8",
    "colors": "^1.4.0",
    "commander": "^7.0.0",
    "inquirer": "^7.3.3",
    "re2": "^1.15.9"
  }
}
